<layout alpha="1" backgroundColor="#FFFFFFFF" fontFamily="Roboto, sans-serif" paddingRight="18" paddingTop="18" paddingBottom="18" paddingLeft="18" name="loging">
    <datasource type="loging" name="loging"/>
    <navigationBar title="Personal Data" hideBackButton="false">
        <leftButtons/>
        <rightButtons/>
    </navigationBar>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="Name" bind="loging.name" selectionMode="DateAndTime"/>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="Lastname" bind="loging.lastname" selectionMode="DateAndTime"/>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="DNI" bind="loging.dni" selectionMode="DateAndTime"/>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="Age" bind="loging.age" selectionMode="DateAndTime"/>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="Telephone" bind="loging.number" selectionMode="DateAndTime"/>
    <textfield wpercentage="1" borderColor="#cccccc" borderWidth="1" cornerRadius="3" alpha="1" fontFamily="Roboto, sans-serif" fontSize="14" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="8" paddingTop="4" paddingBottom="4" paddingLeft="8" placeholder="Email" bind="loging.email1" selectionMode="DateAndTime"/>
    <hcontainer wpercentage="1" alpha="1" fontFamily="Roboto, sans-serif" marginTop="0" marginBottom="0" marginLeft="0" marginRight="0" paddingRight="0" paddingTop="0" paddingBottom="0" paddingLeft="0">
        <label alpha="1" marginTop="0" marginBottom="8" marginLeft="0" marginRight="0" paddingRight="2" paddingTop="2" paddingBottom="2" paddingLeft="2" textColor="#3d464d" fontFamily="Roboto-Bold" fontSize="16" text="Turno" bind="loging.turn"/>
    </hcontainer>
    <button wpercentage=".4" height="40" textAlignment="center" cornerRadius="3000" borderColor="#F0591DFF" alpha="1" backgroundColor="#F0591DFF" fontSize="15" marginTop="4" marginBottom="8" paddingRight="8" paddingTop="8" paddingBottom="8" paddingLeft="8" textColor="#FFFFFFFF" fontFamily="Roboto-Regular" selfAlignment="center" text="Take Turn">
        <selectedState/>
        <highlightedState/>
        <event on="TapEvent">
            <action type="RunRuleAction" ruleContext="MainContext" ruleName="take"/>
        </event>
    </button>
    <button wpercentage="0.4" height="40" textAlignment="center" cornerRadius="3000" borderColor="#F0591DFF" borderWidth="2" alpha="1" backgroundColor="#FFFFFFFF" fontSize="15" marginTop="4" marginBottom="8" paddingRight="8" paddingTop="8" paddingBottom="8" paddingLeft="8" textColor="#F0591DFF" fontFamily="Roboto-Bold" selfAlignment="center" text="Cancel">
        <selectedState/>
        <highlightedState/>
        <event on="TapEvent">
            <action type="NavigateAction" navigationType="dismiss"/>
        </event>
    </button>
</layout>